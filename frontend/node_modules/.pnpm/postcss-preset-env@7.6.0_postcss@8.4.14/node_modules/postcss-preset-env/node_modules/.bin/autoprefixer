#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fabo/programas/next_react/PI-Plataform/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/fabo/programas/next_react/PI-Plataform/frontend/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../autoprefixer@10.4.7_postcss@8.4.14/node_modules/autoprefixer/bin/autoprefixer" "$@"
else
  exec node  "$basedir/../../../../../autoprefixer@10.4.7_postcss@8.4.14/node_modules/autoprefixer/bin/autoprefixer" "$@"
fi
